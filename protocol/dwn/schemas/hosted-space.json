{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://wired-protocol.org/v0/schemas/hosted-space.json",
  "type": "object",
  "properties": {
    "url": {
      "description": "DID URL of the space to host",
      "type": "string"
    },
    "maxPlayers": {
      "description": "Cap the player limit to no larger than this value.",
      "type": "number"
    }
  },
  "required": [
    "url"
  ]
}
