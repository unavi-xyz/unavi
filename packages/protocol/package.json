{
  "name": "@unavi/protocol",
  "version": "0.2.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/unavi-xyz/unavi.git",
    "directory": "packages/protocol"
  },
  "license": "GPL-3.0-or-later",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf src/generated/*",
    "dev": "tsc -w",
    "generate": "pnpm clean && protoc --ts_out src/generated --proto_path protos protos/**/*.proto",
    "lint": "cd ../.. && eslint --fix packages/protocol --ext .ts,.tsx,.mjs,.json --ignore-path .gitignore --ignore-path .prettierignore --ignore-pattern package.json"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.9.1",
    "@wired-protocol/types": "^0.9.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.9.1",
    "eslint": "^8.44.0",
    "eslint-config-custom": "workspace:^",
    "tsconfig": "workspace:^",
    "typescript": "~5.1.3"
  }
}
